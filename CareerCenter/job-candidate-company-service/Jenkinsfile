pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..##################'
                cd genspark-training/CareerCenter/job-candidate-company-service
                bat mvninstall.bat
                    }
        }
        stage('Test') {
            steps {
                echo 'Testing.. #####################'
                mvn test
                    }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
